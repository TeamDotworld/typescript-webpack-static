{
  "name": "webpack-typescript-static",
  "version": "1.0.0",
  "description": "Starter template for static page using webpack and typescript",
  "main": "index.js",
  "scripts": {
    "build": "set NODE_ENV=production && webpack --config config/webpack.config.prod.js",
    "start:dev": "set NODE_ENV=development && webpack-dev-server --open --config config/webpack.config.dev.js"
  },
  "keywords": [
    "webpack",
    "typescript",
    "html"
  ],
  "author": "Naveen Sakthivel",
  "license": "MIT",
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-loader": "^3.0.2",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.8.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "terser-webpack-plugin": "^2.1.3",
    "ts-loader": "^6.2.0",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-merge": "^4.2.2"
  }
}
